<Window x:Class="TPVTAC_WPF.DialogMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DialogMain" Style="{DynamicResource ModalWindowStyle}" Height="300" Width="481">
    <Grid Height="Auto" Width="Auto">
        <Grid.RowDefinitions>
            <RowDefinition Height="15" />
            <RowDefinition Height="*" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center" Width="Auto">
            <Button  Height="50" Style="{DynamicResource btnOK}" Width="50" Margin="10" Visibility="Visible" IsDefault="True"/>
            <Button  Height="50" Style="{DynamicResource btnCancel}" Width="50" Margin="10" Visibility="Visible" IsCancel="True" />
        </StackPanel>
    </Grid>
</Window>
