<Window x:Class="TPVTAC_WPF.DialogMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Aviso" Style="{DynamicResource ModalWindowStyle}" Height="217" Width="432">
    <Grid Height="Auto" Width="Auto">
        <Grid.RowDefinitions>
            <RowDefinition Height="15" />
            <RowDefinition Height="*" />
            <RowDefinition Height="70" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center" Width="Auto" Margin="0,0,0,2">
            <Button  Height="50" Style="{DynamicResource btnOK}" Width="50" Margin="20,0,20,0" Visibility="Visible" IsDefault="True"/>
            <Button  Height="50" Style="{DynamicResource btnCancel}" Width="50" Margin="20,0,20,0" Visibility="Visible" IsCancel="True" />
        </StackPanel>
        <Label x:Name="lblConetindo" Content="Label" Grid.Row="1" Height="Auto" Width="345" HorizontalAlignment="Left" Margin="70,0,70,0" VerticalAlignment="Top" />
    </Grid>
</Window>
